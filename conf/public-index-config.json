{
	"settings":{
		"index":{
			"analysis":{
				"filter":{
					"snowball":{
						"type":"snowball",
						"language":"German2"
					}
					
				},
				"analyzer":{
					"id_analyzer":{
						"filter":[
							"lowercase"
						],
						"tokenizer":"keyword"
					},
					"german_analyzer":{
						"type":"custom",
						"filter":[
							"snowball",
							"icu_folding"
						],
						"tokenizer":"icu_tokenizer"
					}
				}
				
			}
			
		}
		
	},
	"mappings":{
		"monograph":{
			"dynamic_templates":[
				{
					"context":{
						"match":"@context",
						"mapping":{
							"type":"object",
							"enabled":false
						}
						
					}
					
				},
				{
					"dates":{
						"match":"*",
						"match_mapping_type":"date",
						"mapping":{
							"type":"string",
							"index":"not_analyzed"
						}
						
					}
					
				},
				{
					"all":{
						"match":"*",
						"match_mapping_type":"string",
						"mapping":{
							"type":"string",
							"index":"german_analyzer"
						}
						
					}
					
				},
				{
					"@id":{
						"index_analyzer":"id_analyzer",
						"type":"string",
						"search_analyzer":"id_analyzer"
					},
					
				}
				
			]
			
		},
